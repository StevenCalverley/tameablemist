  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.4.0",
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "autoprefixer": "^10.4.0",
    "eslint": "8.4.1",
    "eslint-config-next": "12.0.7",
    "postcss": "^8.4.4",
    "prettier": "^2.5.1",
    "prettier-plugin-prisma": "^3.9.0",
    "prisma": "^3.9.2",
    "tailwindcss": "^3.0.1",
    "typescript": "4.5.3"
